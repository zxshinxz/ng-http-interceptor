{
  "name": "ng-http-interceptor",
  "version": "3.0.0-beta.3",
  "scripts": {
    "prebuild": "rimraf dist compiled",
    "build": "npm run build:main",
    "build:main": "ngc -p tsconfig.aot.json",
    "postbuild:main": "npm run build:umd",
    "build:umd": "rollup -c rollup.config.js",
    "postbuild:umd": "uglifyjs -c --screw-ie8 --comments -o dist/bundles/ng-http-interceptor.umd.min.js dist/bundles/ng-http-interceptor.umd.js",
    "check-coverage": "istanbul check-coverage --functions 75 --lines 75 --branches 75 --statements 75",
    "ci": "npm run lint && npm run test:single && npm run check-coverage",
    "commit": "git-cz",
    "lint": "tslint src/**/*.ts",
    "prepublish": "npm run build",
    "test": "karma start",
    "test:single": "npm run test -- --single-run",
    "test:report": "cd coverage && codecov",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "e2e": "npm-run-all -p -r e2e:server e2e:protractor",
    "e2e:server": "node e2e/demo/server.js",
    "e2e:protractor": "protractor",
    "e2e:update": "webdriver-manager update"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gund/ng-http-interceptor"
  },
  "author": {
    "name": "Alex Malkevich",
    "email": "malkevich.alex@gmail.com"
  },
  "keywords": [
    "ng",
    "ng2",
    "angular",
    "angular2",
    "http",
    "interceptor",
    "library"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gund/ng-http-interceptor/issues"
  },
  "main": "./dist/bundles/ng-http-interceptor.umd.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "peerDependencies": {
    "@angular/core": "4.0.0-beta.7",
    "@angular/http": "4.0.0-beta.7",
    "rxjs": "^5.1.0",
    "tslib": "^1.5.0"
  },
  "devDependencies": {
    "@angular/common": "4.0.0-beta.7",
    "@angular/compiler": "4.0.0-beta.7",
    "@angular/compiler-cli": "4.0.0-beta.7",
    "@angular/core": "4.0.0-beta.7",
    "@angular/forms": "4.0.0-beta.7",
    "@angular/http": "4.0.0-beta.7",
    "@angular/platform-browser": "4.0.0-beta.7",
    "@angular/platform-browser-dynamic": "4.0.0-beta.7",
    "@angular/platform-server": "4.0.0-beta.7",
    "@angular/router": "4.0.0-beta.7",
    "@types/jasmine": "~2.2.30",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "codecov": "^1.0.1",
    "codelyzer": "^2.0.0",
    "commitizen": "^2.9.5",
    "core-js": "^2.4.1",
    "cz-conventional-changelog": "^1.2.0",
    "express": "^4.14.0",
    "ghooks": "^2.0.0",
    "istanbul": "^0.4.5",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.0.0",
    "json-loader": "^0.5.4",
    "karma": "1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-remap-istanbul": "^0.4.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "npm-run-all": "^4.0.0",
    "postcss-loader": "^1.2.1",
    "protractor": "4.0.14",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "rollup": "^0.41.4",
    "rollup-globals-regex": "^0.0.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rxjs": "^5.1.0",
    "semantic-release": "^6.3.2",
    "source-map": "^0.5.6",
    "source-map-loader": "^0.1.5",
    "sourcemap-istanbul-instrumenter-loader": "^0.2.0",
    "ts-node": "^2.0.0",
    "tslib": "^1.5.0",
    "tslint": "^4.4.2",
    "tslint-loader": "^3.3.0",
    "typescript": "^2.1.6",
    "typings": "^2.1.0",
    "uglifyjs": "^2.4.10",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^2.6.1",
    "webpack-sources": "^0.1.4",
    "zone.js": "^0.7.6"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "config": {
    "ghooks": {
      "pre-commit": "echo \"Verifying commit...\" && npm run ci"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
